@all("public")
@create(role:"seller") 
@one("owner") @update("owner") @remove("owner")
type Product @model {
   amountAvailable: Int @default(1)
   cost: Int!
   name: String! @unique
}

type User @model {
    username: String
    deposit: Int @default(0)
}